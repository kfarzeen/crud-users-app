<!DOCTYPE html>
<html>
    <head>
      <title>home page</title>
      <style>
        table,tr,th,td{
            border:2px solid black;
        }
      </style>
    </head>
    <body>
        <table>
            <tr>
            <th>id</th>
            <th>username</th>
            <th>email</th>
            <th>password</th>
            </tr>
            
               <% for(user of users){ %>
                <tr>
               <td><%= user.id %> </td>
               <td><%= user.username %> </td>
               <td><%= user.email %> </td>
               <td><%= user.password %> </td>
               <td>
               <form method="GET" action="/user/<%= user.id %>/edit">
               <button>edit username</button>
               </form></td>
                </tr>
            <%   } %>
                
           
        </table>
    </body>
</html>
